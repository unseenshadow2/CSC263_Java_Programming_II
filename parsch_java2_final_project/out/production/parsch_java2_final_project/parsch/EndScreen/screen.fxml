<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="parsch.EndScreen.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="800.0" prefWidth="1000.0">
         <children>
            <Button layoutX="435.0" layoutY="725.0" mnemonicParsing="false" onAction="#EndGame" text="I'm Outta Here!!!" />
            <Label fx:id="lblLostWon" alignment="CENTER" layoutY="96.0" prefHeight="21.0" prefWidth="1000.0" text="Lost or Won">
               <font>
                  <Font name="Comic Sans MS Bold Italic" size="50.0" />
               </font>
            </Label>
            <Label fx:id="lblTurnCount" alignment="CENTER" layoutY="166.0" prefHeight="21.0" prefWidth="1000.0" text="In 0 Turns!!!">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</GridPane>
